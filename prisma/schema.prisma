generator client {
    provider = "prisma-client-js"
}

datasource db {
    // provider = "mysql"
    // url = env("DATABASE_URL")
    // relationMode = "prisma"
    provider = "sqlite"
    url      = "file:./dev.db"
}

model Schedule {
    id         String   @id @default(uuid())
    day        String
    startTime  String
    endTime    String
    course     String
    subject    String
    instructor String
    room       String
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt
}
