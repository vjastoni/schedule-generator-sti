generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

model Course {
    id  String @id @default(uuid())
    day String
    startTime Int
    endTime Int
    morning Boolean
    course String
    subject String
    instructor String
    room Int
    createdAt  DateTime @default(now())
    updatedAt  DateTime @updatedAt
}